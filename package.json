{
    "name": "@rimac-technology/eslint-plugin",
    "version": "1.4.0",
    "author": "Rimac Technology d.o.o.",
    "description": "Custom eslint rules",
    "license": "MIT",
    "repository": "git@github.com:RimacTechnology/eslint-plugin.git",
    "main": "lib/index.js",
    "scripts": {
        "lint": "eslint './src/**/*' --quiet --cache --cache-strategy content --cache-location '.eslintcache/'",
        "build": "tsc -p tsconfig.production.json",
        "test": "jest --config=./jest.config.ts --runInBand",
        "cm": "cz",
        "release": "semantic-release",
        "postinstall": "husky install",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "lib"
    ],
    "engines": {
        "yarn": ">=3"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@rimac-technology/eslint-config": "^22.2.1",
        "@semantic-release/changelog": "^6.0.1",
        "@semantic-release/commit-analyzer": "^9.0.2",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^8.0.6",
        "@semantic-release/npm": "^9.0.1",
        "@semantic-release/release-notes-generator": "^10.0.3",
        "@types/dedent": "^0.7.0",
        "@types/node": "^18.11.0",
        "@typescript-eslint/parser": "^5.19.0",
        "@typescript-eslint/utils": "^5.40.0",
        "commitizen": "^4.2.5",
        "conventional-changelog-conventionalcommits": "^5.0.0",
        "dedent": "^0.7.0",
        "eslint": "8.25.0",
        "eslint-plugin-eslint-plugin": "^5.0.6",
        "husky": "^8.0.1",
        "jest": "^29.2.0",
        "pinst": "^3.0.0",
        "prettier": "^2.7.1",
        "semantic-release": "^19.0.5",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "packageManager": "yarn@3.2.1",
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
